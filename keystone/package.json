{
  "name": "keystone-app",
  "version": "1.0.2",
  "private": true,
  "license": "MIT",
  "scripts": {
    "resetdb": "keystone dev --reset-db",
    "migrate": "keystone prisma migrate deploy",
    "deploy": "keystone build && yarn migrate",
    "dev": "keystone dev",
    "start": "prisma migrate deploy && keystone start",
    "build": "keystone build",
    "postinstall": "if-env SKIP_POSTINSTALL=1 && echo 'skipped' || keystone postinstall"
  },
  "dependencies": {
    "@keystone-6/auth": "^6.0.0",
    "@keystone-6/cloudinary": "^6.0.0",
    "@keystone-6/core": "^4.0.1",
    "@keystone-6/fields-document": "^6.0.0",
    "dotenv": "^16.0.3",
    "if-env": "^1.0.4",
    "slugify": "^1.6.5",
    "typescript": "^4.8.0"
  }
}
